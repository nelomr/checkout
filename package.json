{
  "name": "checkout",
  "version": "0.1.0",
  "private": true,
  "description": "checkout description",
  "author": "nelomr9@gmail.com",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "postcss-apply": "^0.12.0",
    "postcss-custom-media": "^7.0.8",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.0",
    "@vue/cli-plugin-eslint": "^4.2.0",
    "@vue/cli-service": "^4.2.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "vue-cli-plugin-postcss-precss": "^1.0.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "postcss": {
    "plugins": {
      "postcss-assets": {
        "cachebuster": false,
        "loadPaths": [
          "public/"
        ]
      },
      "postcss-import": {},
      "postcss-extend-rule": {},
      "postcss-apply": {},
      "postcss-advanced-variables": {},
      "postcss-plugin-bem-atrule": {
        "defaultNamespace": "el",
        "style": "suit",
        "separators": {
          "descendent": "__",
          "modifier": "--"
        },
        "shortcuts": {
          "utility": "util",
          "component": "b",
          "modifier": "m",
          "descendent": "e"
        }
      },
      "postcss-preset-env": {
        "stage": 2
      },
      "postcss-custom-media": {},
      "postcss-atroot": {},
      "postcss-property-lookup": {},
      "postcss-nested": {},
      "postcss-nested-ancestors": {},
      "postcss-calc": {},
      "postcss-sass-color-functions": {},
      "lost": false,
      "postcss-utils": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
